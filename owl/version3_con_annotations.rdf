    <!-- http://www.semanticweb.org/daniel/ontologies/2025/9/ontologia_tfg#C62310 -->

    <owl:Class rdf:about="http://www.semanticweb.org/daniel/ontologies/2025/9/ontologia_tfg#C62310">
