        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2025/9/ontologia_tfg#C2124"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/daniel/ontologies/2025/9/ontologia_tfg#C1512 -->

    <owl:Class rdf:about="http://www.semanticweb.org/daniel/ontologies/2025/9/ontologia_tfg#C1512">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2025/9/ontologia_tfg#C307"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/daniel/ontologies/2025/9/ontologia_tfg#C308"/>
